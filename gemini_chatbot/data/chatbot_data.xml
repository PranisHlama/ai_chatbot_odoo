<odoo>
    <data noupdate="1">

        <!-- Create a partner for the chatbot -->
        <record id="gemini_chatbot_chatbot_partner" model="res.partner">
            <field name="name">Gemini Chatbot</field>
            <field name="email">gemini@email.com</field>
            <field name="is_company">False</field>
            <field name="active">True</field>
            <field name="user_ids" eval="[(5,0,0)]"/>
        </record>

        <!-- Create a system user linked to that partner -->
        <record id="gemini_chatbot_chatbot_user" model="res.users">
            <field name="login">gemini_chatbot</field>
            <field name="partner_id" ref="gemini_chatbot_chatbot_partner"/>
            <field name="password">123</field>
            <field name="active">True</field>
        </record>

    </data>
</odoo>
